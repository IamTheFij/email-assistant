[DEFAULT]
modules_path =  # No value, defaults to using weboob-modules Python module

[first_module_name]
# For each backend, you should provide the required configuration options to
# build the backend in a dedicated section.
backend = WEBOOB_MODULE_NAME
login = LOGIN
password = PASSWORD

[other_module_name]
# You can surround a value with quotes "`" to run a subcommand to get the
# results, here `pass`.
backend = OTHER_WEBOOB_MODULE_NAME
login = `pass show other_module/login`
password = `pass show other_module/password`
